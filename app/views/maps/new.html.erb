<h1>New map</h1>

<% form_for(@map) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :width %><br />
    <%= f.text_field :width %>
  </p>
  <p>
    <%= f.label :height %><br />
    <%= f.text_field :height %>
  </p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', maps_path %>